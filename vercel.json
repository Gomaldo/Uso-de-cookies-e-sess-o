{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/login", "dest": "/app.js" },
    { "src": "/cadastro", "dest": "/app.js" },
    { "src": "/tabela", "dest": "/app.js" }
  ]
}
